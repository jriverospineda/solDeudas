<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app	xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-web-app" 
						xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
						xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://xmlns.oracle.com/weblogic/weblogic-web-app http://xmlns.oracle.com/weblogic/weblogic-web-app/1.3/weblogic-web-app.xsd">
    <wls:weblogic-version>10.3.6</wls:weblogic-version>
    
    <wls:security-role-assignment>
    	<wls:role-name>clienteHabitat</wls:role-name>
    	<wls:principal-name>clienteHabitat</wls:principal-name>
    </wls:security-role-assignment>
    
    <wls:security-role-assignment>
    	<wls:role-name>G_Habitat_do2_ROL_SolDeudas_Consulta CERT</wls:role-name>
    	<wls:principal-name>G_Habitat_do2_ROL_SolDeudas_Consulta CERT</wls:principal-name>
    </wls:security-role-assignment>
    
    <wls:security-role-assignment>
    	<wls:role-name>G_Habitat_do2_ROL_SolDeudas_Consulta</wls:role-name>
    	<wls:principal-name>G_Habitat_do2_ROL_SolDeudas_Consulta</wls:principal-name>
    </wls:security-role-assignment>
        
    <wls:session-descriptor>
    	<wls:timeout-secs>72000</wls:timeout-secs>
    	<wls:persistent-store-type>replicated_if_clustered</wls:persistent-store-type>
    </wls:session-descriptor>
    
    <wls:jsp-descriptor>
    	<wls:encoding>ISO8859-1</wls:encoding>
    </wls:jsp-descriptor>
	
    <wls:context-root>solDeudasWeb</wls:context-root>
    
</wls:weblogic-web-app>

