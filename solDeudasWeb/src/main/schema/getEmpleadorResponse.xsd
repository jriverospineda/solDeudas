					<xsd:element ref="emp:codigoRespuesta"/>
					<xsd:element ref="emp:descripcionRespuesta"/>
		      </xsd:sequence>
		    </xsd:complexType>
		  </xsd:element>
 		  <xsd:element name="condicion">
		    <xsd:complexType>
		      <xsd:sequence>
			        <xsd:element ref="emp:codigoRespuesta"/>
			        <xsd:element ref="emp:descripcionRespuesta"/>
		      </xsd:sequence>
		    </xsd:complexType>
		  </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>