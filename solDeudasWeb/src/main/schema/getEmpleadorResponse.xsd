<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://afphabitat.cl/soap/empleador" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:emp="http://afphabitat.cl/soap/empleador">
